# Unidentified files
application/octet-stream; ~/.mutt/view_attachment.sh %s "-"

#text/html; ~/.mutt/view_attachment.sh %s '-' ; nametemplate=%s.html
text/html; links %s ; nametemplate=%s.html
text/html; links -dump %s ; nametemplate=%s.html ; copiousoutput
image/*; ~/.mutt/view_attachment.sh %s "-"

# QuickLook in OSX
audio/*                  ; /usr/bin/qlmanage -p 2>/dev/null %s
video/*                  ; /usr/bin/qlmanage -p 2>/dev/null %s
application/*            ; /usr/bin/qlmanage -p 2>/dev/null %s
